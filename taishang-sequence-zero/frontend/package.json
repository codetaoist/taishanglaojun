{
  "name": "taishang-sequence-zero-frontend",
  "version": "0.1.0",
  "private": true,
  "description": "太上老君序列0前端应用 - 现代化的React用户界面",
  "homepage": ".",
  "dependencies": {
    "@ant-design/icons": "^5.2.6",
    "@craco/craco": "^7.1.0",
    "@emotion/react": "^11.11.1",
    "@emotion/styled": "^11.11.0",
    "@reduxjs/toolkit": "^1.9.7",
    "@types/node": "^20.8.7",
    "@types/react": "^18.2.31",
    "@types/react-dom": "^18.2.14",
    "antd": "^5.11.1",
    "axios": "^1.5.1",
    "dayjs": "^1.11.10",
    "framer-motion": "^10.16.4",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-redux": "^8.1.3",
    "react-router-dom": "^6.17.0",
    "react-scripts": "5.0.1",
    "recharts": "^2.8.0",
    "redux-persist": "^6.0.0",
    "socket.io-client": "^4.7.2",
    "styled-components": "^6.1.0",
    "typescript": "^5.2.2",
    "web-vitals": "^3.5.0"
  },
  "scripts": {
    "start": "craco start",
    "build": "craco build",
    "test": "craco test",
    "eject": "react-scripts eject",
    "lint": "eslint src --ext .ts,.tsx --fix",
    "format": "prettier --write src/**/*.{ts,tsx,css,scss}",
    "analyze": "pnpm run build && pnpx bundle-analyzer build/static/js/*.js",
    "serve": "serve -s build -l 3000",
    "dev": "pnpm start",
    "install:clean": "rm -rf node_modules pnpm-lock.yaml && pnpm install",
    "update:deps": "pnpm update --latest"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ],
    "rules": {
      "@typescript-eslint/no-unused-vars": "warn",
      "react-hooks/exhaustive-deps": "warn"
    }
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@types/styled-components": "^5.1.29",
    "@typescript-eslint/eslint-plugin": "^6.9.0",
    "@typescript-eslint/parser": "^6.9.0",
    "eslint": "^8.52.0",
    "eslint-plugin-react": "^7.33.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "prettier": "^3.0.3",
    "serve": "^14.2.1"
  },
  "proxy": "http://localhost:8080",
  "engines": {
    "node": ">=16.0.0",
    "npm": ">=8.0.0"
  },
  "keywords": [
    "taishang",
    "sequence-zero",
    "react",
    "typescript",
    "antd",
    "consciousness",
    "cultural-wisdom"
  ],
  "author": {
    "name": "Taishang Team",
    "email": "dev@taishang.ai"
  },
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/taishang/sequence-zero.git",
    "directory": "frontend"
  }
}