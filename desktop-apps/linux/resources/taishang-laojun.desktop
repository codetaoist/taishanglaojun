Exec=taishang-laojun --new-chat

[Desktop Action NewProject]
Name=New Project
Name[zh_CN]=新建项目
Name[zh_TW]=新建項目
Exec=taishang-laojun --new-project

[Desktop Action Settings]
Name=Settings
Name[zh_CN]=设置
Name[zh_TW]=設置
